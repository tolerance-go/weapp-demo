{
   "name": "yorkbar",
   "version": "1.0.1",
   "private": true,
   "description": "",
   "keywords": [],
   "license": "ISC",
   "author": "yarnb@foxmail.com",
   "scripts": {
      "clean:node_modules": "rimraf packages/*/node_modules dependents/*/node_modules apps/*/node_modules node_modules",
      "changeset": "changeset",
      "clean": "turbo run clean",
      "db:generate": "turbo run db:generate",
      "db:seed": "turbo run db:seed",
      "dev:mandong-admin": "turbo run dev --filter=@yorkbar/mandong-admin",
      "dev:mandong-blog": "turbo run dev --filter=@yorkbar/mandong-blog",
      "dev:mandong-core": "turbo run dev --filter=@yorkbar/mandong-core",
      "dev:mandong-docs": "turbo run dev --filter=@yorkbar/mandong-docs",
      "dev:mandong-hero": "turbo run dev --filter=@yorkbar/mandong-hero",
      "dev:mandong-ide": "turbo run dev --filter=@yorkbar/mandong-ide",
      "dev:mandong-weapp": "turbo run dev --filter=@yorkbar/mandong-weapp",
      "dev:ui": "turbo run dev --filter=@yorkbar/ui",
      "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
      "lint": "turbo run lint",
      "next-version": "tsx ./scripts/next-version.ts",
      "publish-packages": "turbo run build lint test && changeset version && changeset publish",
      "version-packages": "changeset status --output=./next-version.json && changeset version && npm run next-version"
   },
   "devDependencies": {
      "@changesets/cli": "^2.25.2",
      "@yorkbar/eslint-config-custom": "workspace:*",
      "@yorkbar/scripts": "workspace:*",
      "eslint": "^7.29.0",
      "prettier": "^2",
      "prettier-plugin-organize-imports": "^3.2.0",
      "prettier-plugin-packagejson": "^2",
      "rimraf": "^3.0.2",
      "semver": "^7.3.8",
      "tsx": "^3.12.1",
      "turbo": "^1.6.3",
      "zx": "^7.1.1"
   }
}
