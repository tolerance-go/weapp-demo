{
   "printWidth": 80,
   "singleQuote": true,
   "jsxSingleQuote": true,
   "trailingComma": "all",
   "tabWidth": 3,
   "semi": false,
   "bracketSpacing": true,
   "overrides": [
      {
         "files": ".prettierrc",
         "options": { "parser": "json" }
      }
   ],
   "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-packagejson"
   ]
}
